package recursion;

import java.util.Scanner;

public class _10872 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int N = sc.nextInt();
		
		factorial(N);
	}

	private static int factorial(int n) {
		// TODO Auto-generated method stub
		if(n == 0) {
			return 1;
		}
		return n * factorial(n-1);
	}
}
